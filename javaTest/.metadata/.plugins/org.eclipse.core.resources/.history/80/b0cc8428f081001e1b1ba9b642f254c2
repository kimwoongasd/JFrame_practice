package com.sist.vo;

public class bookVo {
	int no, price;
	String bookName, publisher;
	
	
	public bookVo(int no, int prive, String bookName, String publisher) {
		super();
		this.no = no;
		this.prive = prive;
		this.bookName = bookName;
		this.publisher = publisher;
	}
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public int getPrive() {
		return prive;
	}
	public void setPrive(int prive) {
		this.prive = prive;
	}
	public String getBookName() {
		return bookName;
	}
	public void setBookName(String bookName) {
		this.bookName = bookName;
	}
	public String getPublisher() {
		return publisher;
	}
	public void setPublisher(String publisher) {
		this.publisher = publisher;
	}
	

}
